{
  "name": "bogie",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "postinstall": "./node_modules/.bin/gulp babel",
    "test": "./node_modules/.bin/xo src/**/*.js web/**/*.js index.android.js index.ios.js index.web.js"
  },
  "dependencies": {
    "babel-polyfill": "^6.9.0",
    "babel-relay-plugin": "^0.7.3",
    "body-parser": "^1.14.2",
    "cookie-parser": "^1.4.0",
    "express": "^4.13.3",
    "fbjs": "^0.8.3",
    "history": "^1.17.0",
    "isomorphic-fetch": "^2.2.0",
    "isomorphic-relay": "^0.7.0-beta.0",
    "isomorphic-relay-router": "^0.8.0-beta.0",
    "moment": "^2.10.6",
    "morgan": "^1.6.1",
    "react": "^15.1.0",
    "react-autosuggest": "^3.7.4",
    "react-cookie": "^0.4.6",
    "react-dom": "^0.14.3",
    "react-helmet": "^3.1.0",
    "react-native": "^0.27.1",
    "react-native-scrollable-tab-view": "^0.4.3",
    "react-native-vector-icons": "^1.3.4",
    "react-native-web": "0.0.25",
    "react-proxy": "^1.1.8",
    "react-relay": "^0.9.0",
    "react-router": "^2.0.0-rc5",
    "react-router-relay": "^0.9.0",
    "react-slider": "^0.6.1",
    "url-loader": "^0.5.7",
    "zxcvbn": "^4.2.0"
  },
  "xo": {
    "esnext": true,
    "envs": [
      "browser",
      "commonjs"
    ],
    "plugins": [
      "react"
    ],
    "extends": [
      "xo-react"
    ],
    "globals": [
      "cssInJS"
    ],
    "space": 4,
    "rules": {
      "react/jsx-indent-props": 0,
      "react/jsx-closing-bracket-location": 0,
      "camelcase": 1,
      "new-cap": 1,
      "linebreak-style": 0
    }
  },
  "pre-commit": [
    "test"
  ],
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.9.0",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-native": "^1.5.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-relay-plugin": "^0.9.0",
    "browser-sync": "^2.12.8",
    "eslint": "^1.10.3",
    "eslint-config-xo": "^0.9.1",
    "eslint-config-xo-react": "^0.3.0",
    "eslint-plugin-babel": "^3.2.0",
    "eslint-plugin-no-empty-blocks": "0.0.2",
    "eslint-plugin-no-use-extend-native": "^0.3.4",
    "eslint-plugin-react": "^3.14.0",
    "gulp": "^3.9.0",
    "gulp-babel": "^6.1.1",
    "gulp-changed": "^1.3.0",
    "piping": "1.0.0-rc.2",
    "pre-commit": "^1.1.3",
    "react-hot-loader": "^3.0.0-beta.2",
    "react-native-cli": "^0.2.0",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0",
    "webpack-stream": "^3.2.0",
    "xo": "^0.12.1"
  },
  "engines": {
    "node": ">=5.0.0"
  }
}
